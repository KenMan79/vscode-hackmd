{
	"name": "vscode-hackmd",
	"displayName": "HackMD",
	"description": "The official HackMD Markdown VSCode extension",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.36.0"
	},
	"publisher": "HackMD",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#333333",
		"theme": "dark"
	},
	"activationEvents": [],
	"license": "MIT",
	"homepage": "https://github.com/hackmdio/vscode-hackmd/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/hackmdio/vscode-hackmd/issues",
		"email": "hackmdio@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hackmdio/vscode-hackmd.git"
	},
	"extensionDependencies": [
		"bierner.markdown-preview-github-styles",
		"bierner.markdown-emoji",
		"bierner.emojisense",
		"bierner.markdown-image-size",
		"bierner.markdown-footnotes",
		"bierner.markdown-checkbox"
	],
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"markdown.markdownItPlugins": true,
		"markdown.previewScripts": [
			"./dist/page.js"
		],
		"markdown.previewStyles": [
			"./dist/page.css"
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch",
		"test-compile": "tsc -p ./",
		"pretest": "npm run compile",
		"test": "node ./dist/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jquery": "^3.3.30",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"abcjs": "^5.8.0",
		"css-loader": "^3.1.0",
		"flowchart.js": "^1.12.2",
		"glob": "^7.1.4",
		"highlight.js": "^9.15.8",
		"ignore-loader": "^0.1.2",
		"imports-loader": "^0.8.0",
		"jquery": "^3.4.1",
		"js-sequence-diagrams": "git+https://github.com/Moeditor/js-sequence-diagrams.git",
		"katex": "^0.10.2",
		"lodash": "^4.17.15",
		"mermaid": "^8.2.3",
		"mini-css-extract-plugin": "^0.8.0",
		"mocha": "^6.1.4",
		"prismjs": "^1.17.1",
		"raphael": "git+ssh://git@github.com/DmitryBaranovskiy/raphael.git#v2.2.7",
		"script-loader": "^0.7.2",
		"style-loader": "^0.23.1",
		"ts-loader": "^6.0.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"viz.js": "^2.1.2",
		"vscode-test": "^1.0.0-next.0",
		"webpack": "^4.37.0",
		"webpack-cli": "^3.3.6"
	},
	"dependencies": {
		"bootstrap3": "^3.3.5",
		"markdown-it-abbr": "^1.0.4",
		"markdown-it-container": "^2.0.0",
		"markdown-it-deflist": "^2.0.3",
		"markdown-it-ins": "^2.0.0",
		"markdown-it-mark": "^2.0.0",
		"markdown-it-mathjax": "^2.0.0",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-table-of-contents": "^0.4.4",
		"string": "git+ssh://git@github.com/hackmdio/string.js.git#a68176b3d"
	}
}
